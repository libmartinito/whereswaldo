{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb",
      "prettier"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
      "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
      "react/prop-types": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
